---
import 'styles/globals.css';

import AppLayout from '@/components/app/AppLayout';
import Head from 'layouts/components/Head.astro';

const { description } = Astro.props

export type Props = {
  title?: string
  description?: string
}

---

<!doctype html>
<html lang="en">

<Head description={description} />
<AppLayout client:visible>
  <slot />
</AppLayout>

</html>
